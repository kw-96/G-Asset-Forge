# 开发环境配置
NODE_ENV=development

# Electron 开发配置
ELECTRON_ENABLE_LOGGING=true
ELECTRON_ENABLE_STACK_DUMPING=true

# Webpack 开发服务器配置
WEBPACK_DEV_SERVER_PORT=3000
WEBPACK_DEV_SERVER_HOST=localhost

# 调试配置
DEBUG_PROD=false
DEBUG_DEV=true

# 日志级别
LOG_LEVEL=debug

# 安全配置（开发环境）
ALLOW_DEVTOOLS=true
ALLOW_LOCALHOST=true

# 性能配置
WEBPACK_CACHE=true
TS_TRANSPILE_ONLY=true